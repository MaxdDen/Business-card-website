{
  "name": "business-card-cms",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build:css": "tailwindcss -i ./app/static/css/input.css -o ./app/static/css/output.css",
    "build:css-minify": "tailwindcss -i ./app/static/css/input.css -o ./app/static/css/output.css --minify",
    "build:css-watch": "tailwindcss -i ./app/static/css/input.css -o ./app/static/css/output.css --watch",
    "tailwind:build": "npx @tailwindcss/cli -i ./app/static/css/input.css -o ./app/static/css/output.css",
    "tailwind:watch": "npx @tailwindcss/cli -i ./app/static/css/input.css -o ./app/static/css/output.css --watch"
  },
  "devDependencies": {
    "tailwindcss": "^4.1.0",
    "@tailwindcss/cli": "^4.1.0"
  }
}

